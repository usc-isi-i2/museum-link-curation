PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs:<http://www.w3.org/2000/01/rdf-schema#>
PREFIX crm: <http://www.cidoc-crm.org/cidoc-crm/>
SELECT ?uri ?name
    WHERE 
    { 
        GRAPH <http://data.americanartcollaborative.org/nmwa>
        {
            ?uri a crm:E39_Actor;
                 rdfs:label ?name.

        }
    }